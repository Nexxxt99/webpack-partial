{
  "name": "webpack-partial",
  "version": "1.1.2",
  "author": "Izaak Schroeder <izaak.schroeder@gmail.com>",
  "description": "Webpack partials.",
  "license": "CC0-1.0",
  "main": "dist/lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "prepublish": "./node_modules/.bin/babel -s -d dist/lib lib",
    "spec": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register -r adana-dump -R spec test/spec",
    "test": "npm run lint && npm run spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izaakschroeder/webpack-partial"
  },
  "bugs": {
    "url": "https://github.com/izaakschroeder/webpack-partial/issues"
  },
  "keywords": [
    "webpack",
    "partial",
    "include"
  ],
  "dependencies": {
    "lodash": "^4.0.1"
  },
  "devDependencies": {
    "adana-cli": "^0.1.1",
    "adana-dump": "^0.1.0",
    "adana-format-lcov": "^0.1.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-metalab": "^0.2.0",
    "eslint": "^1.10.3",
    "eslint-config-metalab": "^2.0.0-beta.1",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.16.0",
    "mocha": "^2.3.4",
    "chai": "^3.2.0"
  }
}
